<span class="span6">
	<div class="formbox" id="register">
		<h1 class="headtext text-left"><strong class="special">60</strong> days no obligation trial</h1>
		<h3 class="semi-headtext">Registration in less than 25 seconds</h3>
		<% @user = User.new %>
		<%= render :partial => 'devise/registrations/new' %>
	</div>
</span>
<span class="span1 breakvertical">
				</span>
<span class="span5">
	<div class="formbox" id="login">
		<h1 class="headtext text-left">Login</h1>
		<div style="height: 68px"></div>
		<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
			
			<label>
				<%= f.email_field :email, {:class => "input-block-level", :placeholder => "Email"} %>
			</label>
			<label>
				<%= f.password_field :password, {:class => "input-block-level", :placeholder => "Password"} %>
			</label>
			<!-- <label>
				<%= f.check_box :remember_me %> <%= f.label :remember_me %>
			</label> -->
			
			<label>
				<span class="row-fluid">
					<%= f.submit "Manager your visitor", :class => "btn btn-large btn-success span8 offset2", :name=>"loginbt" %>
				</span>
				<br/>
				<span class="row-fluid">
					<a href="#" class="resetpass span8 offset2 text-center">Reset password</a>
				</span>
			</label>
				
		<% end %>
	</div>
</span>
			

