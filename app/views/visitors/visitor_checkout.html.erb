<script type="text/javascript">
	$(document).ready(function() {
		$('#link-search').click(function() {
			$('.text').hide();
			$(this).hide();
			$('#frmVisitorSearch').show();
		});
	});
</script>

<div class="checkoutpage pagerbg">
	<%= form_tag visitor_checkout_path, {:class => 'form-horizontal', :name => "checkoutform"} do %>
		<div class="row-fluid">
			<div class="span4 offset4 text-center">
				<label>
					<%= text_field_tag 'pass_id', '', :class => 'input-block-level passid', :placeholder => "Enter pass id generated by 34Pass" %>
                  
                </label><br>
                
                <label class="or">
                  <p>OR</p>
                </label><br>
                
                <label>
                	<%= text_field_tag 'badge_number', '', :class => 'input-block-level passid companyname', :placeholder => "Enter badge number" %>
                  
                </label><br>
                
                <label>
                  <div class="row-fluid"><span class="span4 offset4">
                  	<input type="submit" class="btn btn-success btn-block" value="Checkout"/>
                 </span></div>
                </label>
			</div>
		</div>
	<% end %>
</div>

<div id="results">
	
</div>
