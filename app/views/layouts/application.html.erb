<!DOCTYPE html>
<html>
	<head>
		<title>Visitor Management</title>
		
		<%= stylesheet_link_tag    "bootstrap.min", :media => "all" %>
		<%= stylesheet_link_tag    "style", :media => "all" %>
		<%= stylesheet_link_tag "fixcss" %>

		<%#= javascript_include_tag "jquery-2.0.0.min" %>
		<%#= javascript_include_tag "bootstrap.min" %>
		<%= javascript_include_tag "application" %>

  		
	  	<%= javascript_include_tag 'webcam' %>
	  	<%= javascript_include_tag "custom" %>
	  	<%= csrf_meta_tags %>
	  	
		<!--[if lt IE 9]>
		<script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
		<![endif]-->
	</head>
	<body>
		
		<div class="header" style="position:fixed; width:100%; background-color:white; height:160px; z-index:10;">
	      <div class="container">
	        <div class="row-fluid">
	        	<span class="span2">
		            <div class="logoheader">
		            	<%= link_to image_tag("logo.png", :alt => "34pass.com"), root_path %>
		            	
	            	</div>
            	</span>
	            <span class="span6 pull-right">
		            <div class="quicksearch">
		              <%= form_tag visitor_search_path do %>
                    <%= select_tag 'search_keyword', options_for_select( [ ["Now Inside", "now_inside"],
                                                                    ["Visitor name", "visitor_name"],
                                                                    ["Here to meet", "here_to_meet"],
                                                                    ["Visitor Organization", "visitor_company_name"],
                                                                    ["Badge ID", "badge_number"],
                                                                    ["34pass ID", "pass_id"], ["Mobile number", "visitor_mobile_number"]
                                                                    ]), class: "search_dropdown span4" %>
                    <%= text_field_tag 'search_key', "", {:class => "quicksearchbar input-block-level span8", :placeholder => "Search..", :style => "margin-top:-10px;"} %>
                    <%= submit_tag '', :class => 'searchbt' %>
                  <% end %>
		            </div>
	            </span>
              <div class="clear"></div>
            	<span class="span7 navigation">
		            <div class="mainmenu">
		              <ul class="unstyled">
		                <li><%= link_to "Check In", new_visitor_path %></li>
		                <li><%= link_to "Checkout", visitor_checkout_path %></li>
		                <li><%= link_to "Expired", twelve_plus_path %></li>
		                <li><%= link_to "Dashboard", dashboard_index_path %></li>
                    <li class="dropdown">
                      <a href="#" class="dropdown-toggle" data-toggle="dropdown">Me</a>
                      <ul class="dropdown-menu">
                        <li><%= link_to "Me", me_index_path %></li>
                        <li><%= link_to "Logout", destroy_user_session_path, :method => :delete %></li>
                      </ul>
                    </li>
		              </ul>
		            </div>
	            </span>
            </div>
	      </div>
	    </div>
	    
	    <div class="bodier">
	    	<div class="container" style="padding-top:180px;">
	    		<%= render partial: "layouts/flash_messages", flash: flash %>
	    		
	    		<%= yield %>
	    	</div>
    	</div>
		
		
	</body>
</html>

<script>
	(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
	(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
	m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
	})(window,document,'script','//www.google-analytics.com/analytics.js','ga');
	ga('create', 'UA-41638664-1', '34pass.com');
	ga('send', 'pageview');
</script>

